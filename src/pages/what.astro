---
import certifications from '@collections/en/certifications.json'
import Layout from '@layouts/main.astro'
import WhatBase from "@components/page/what.astro";
import { getLangFromUrl, useTranslations } from '@i18n/utils'
const lang = getLangFromUrl(Astro.url)
const t = useTranslations(lang)
const prefixLocale = false
---
<!--
import experiences from '@collections/en/experiences.json'
-->
<Layout language="en" title={t('what.title')} prefixLocale={prefixLocale}>
  <WhatBase certifications={certifications}/>
</Layout>
