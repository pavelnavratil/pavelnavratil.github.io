---
import worked from "@collections/cs/worked.json";
import Layout from "@layouts/main.astro";
import WorkedBase from '@components/page/worked.astro'
import { getLangFromUrl, useTranslations } from '@i18n/utils';
const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
const prefixLocale = true;
---

<Layout language="cs" title={t('worked.title')} prefixLocale={prefixLocale}>
  <WorkedBase worked={worked}/>
</Layout>
