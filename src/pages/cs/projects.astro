---
import projects from "@collections/cs/projects.json";
import Layout from "@layouts/main.astro";
import ProjectsBase from '@components/page/projects.astro'
import { getLangFromUrl, useTranslations } from '@i18n/utils';
const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
const prefixLocale = true;
---

<Layout language="cs" title={t('projects.title')} prefixLocale={prefixLocale}>
  <ProjectsBase projects={projects}/>
</Layout>
